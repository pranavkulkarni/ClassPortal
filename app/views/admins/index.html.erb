<%= render 'shared/navigation' %>
<p id="notice"><%= notice %></p>
<div class="panel panel-default">

	<div class="panel-heading">Listing Admins</div>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <!-- <th>Password</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.name %></td>
        <td><%= admin.email %></td>
        <!--<td><%= admin.password %></td> -->
        <!--<td><%= link_to 'Show', admin %></td> -->
        <% if admin.id == session[:current_user_id] %>
        <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
        <% end %>
        <% if admin.email != 'pkulkar5@ncsu.edu' and admin.id != session[:current_user_id]%>
        <td><%= link_to 'Destroy', '/admins/land/destroy/'+ admin.id.to_s%></td>
        <% end %>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<br/>

</div>
<a class="btn btn-lg btn-lg btn-block" href="<%= new_admin_path %>" >New Admin</a>
